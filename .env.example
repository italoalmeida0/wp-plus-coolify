# domínio atribuído pelo Coolify (automaticamente preenchido em SERVICE_FQDN_WORDPRESS)
SERVICE_FQDN_WORDPRESS=

# usuário de acesso ao MySQL (pode ser fixo)
SERVICE_USER_WORDPRESS=wpuser

# deixe em branco para usar o botão “Generate” do Coolify
SERVICE_PASSWORD_WORDPRESS=
SERVICE_PASSWORD_ROOT=
SERVICE_PASSWORD_CODESERVER=

# prefixo de tabela WP (opcional—se vazio cai em wp_)
WORDPRESS_TABLE_PREFIX=
